{
  "root": "libs/utility/lens",
  "sourceRoot": "libs/utility/lens/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/utility/lens",
        "main": "libs/utility/lens/src/index.ts",
        "tsConfig": "libs/utility/lens/tsconfig.lib.json",
        "assets": ["libs/utility/lens/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/utility/lens/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/utility/lens"],
      "options": {
        "jestConfig": "libs/utility/lens/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
