{
  "root": "libs/record",
  "sourceRoot": "libs/record/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/record",
        "main": "libs/record/src/index.ts",
        "tsConfig": "libs/record/tsconfig.lib.json",
        "assets": ["libs/record/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/record/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/record"],
      "options": {
        "jestConfig": "libs/record/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
